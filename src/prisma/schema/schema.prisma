generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model HealthCheck {
  id Int @id @default(autoincrement())
}

model Schedule {
  id Int @id @default(autoincrement())

  account     String
  destination String

  amount Int

  date DateTime @default(now())
}
